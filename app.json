{
  "configVersion": "v3",
  "app": {
    "appId": 20001,
    "appName": "心率位置同步",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.0.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "心率与地理位置实时同步小程序"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.local_storage",
    "device:os.sensor.heart_rate",
    "device:os.geolocation",
    "device:os.bg_service",
    "data:user.hd.heart_rate",
    "data:user.location"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "4.0",
      "target": "4.0",
      "minVersion": "4.0"
    }
  },
  "targets": {
    "gt": {
      "module": {
        "page": {
          "pages": [
            "page/gt/home/index.page"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "zh-CN": {
      "appName": "心率位置同步"
    },
    "en-US": {
      "appName": "Heart Rate Location Sync"
    }
  },
  "defaultLanguage": "zh-CN",
  "debug": false
}
